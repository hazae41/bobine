{
  "type": "module",
  "name": "@hazae41/bobine",
  "version": "0.0.17",
  "description": "A blockchain in your garage",
  "repository": "github:hazae41/bobine",
  "author": "hazae41",
  "license": "MIT",
  "scripts": {
    "examine": "deno lint ./src && deno check ./src && deno test -R ./src",
    "prepack": "rm -rf ./out && tsc && tscousin",
    "produce": "deno run -A --sloppy-imports ./src/mod.ts serve --env=./.env.local",
    "develop": "deno run -A --sloppy-imports ./src/mod.ts serve --env=./.env.local --dev"
  },
  "files": [
    "./out"
  ],
  "exports": {
    ".": {
      "types": "./out/mod.d.ts",
      "import": "./out/mod.js"
    }
  },
  "dependencies": {
    "@hazae41/binary": "^2.0.2",
    "@hazae41/cursor": "^2.1.3",
    "@hazae41/jsonrpc": "^1.1.11",
    "@hazae41/mutex": "^3.0.1",
    "@hazae41/wasm": "^1.0.0",
    "@tursodatabase/database": "^0.3.2"
  },
  "devDependencies": {
    "@hazae41/phobos": "^2.0.16",
    "@hazae41/tscousin": "^1.0.28",
    "@types/node": "^25.0.2",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "jsonrpc",
    "webassembly",
    "typescript",
    "esmodules",
    "tested",
    "unit-tested"
  ],
  "bin": {
    "bobine": "./out/mod.js"
  }
}