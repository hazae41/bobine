{
  "type": "module",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "examine": "deno lint ./src && deno check ./src && deno test ./src",
    "version": "deno -RW ./x.vsync.ts && git add deno.json",
    "prepare": "deno -RW ./x.dsync.ts && deno install",
    "prepack": "glace ./www --out=./out",
    "produce": "glace ./www --out=./out --watch=./src,./www & deno -RNE ./src/mods/srv/bin.ts",
    "develop": "NODE_OPTIONS=--max_old_space_size=4096 glace ./www --out=./out --dev --watch=./src,./www & deno -RNE ./src/mods/srv/bin.ts --dev"
  },
  "files": [
    "./out"
  ],
  "dependencies": {
    "@hazae41/disposable-stack-polyfill": "^2.0.4",
    "@hazae41/immutable": "^2.0.19",
    "@hazae41/jsonrpc": "^1.1.11",
    "@hazae41/labase": "^1.0.16",
    "@hazae41/rewind": "^2.3.12",
    "@heroicons/react": "^2.2.0",
    "assemblyscript": "^0.28.9",
    "highlight.js": "^11.11.1",
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  },
  "devDependencies": {
    "@hazae41/glace": "^1.0.24",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3"
  }
}
